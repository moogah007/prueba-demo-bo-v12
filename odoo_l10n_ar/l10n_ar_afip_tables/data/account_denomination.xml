            <field name="validate_supplier">True</field>
            <field name="description">Comprobantes tipo A</field>
            <field name="vat_discriminated">True</field>
        </record>

        <record id="account_denomination_b" model="account.denomination">
            <field name="name">B</field>
            <field name="validate_supplier">True</field>
            <field name="description">Comprobantes tipo B</field>
        </record>

        <record id="account_denomination_c" model="account.denomination">
            <field name="name">C</field>
            <field name="validate_supplier">True</field>
            <field name="description">Comprobantes tipo C</field>
        </record>

        <record id="account_denomination_d" model="account.denomination">
            <field name="name">D</field>
            <field name="validate_supplier">False</field>
            <field name="description">Despachos de importacion</field>
        </record>

        <record id="account_denomination_e" model="account.denomination">
            <field name="name">E</field>
            <field name="validate_supplier">False</field>
            <field name="description">Documentos de exportacion</field>
        </record>

        <record id="account_denomination_i" model="account.denomination">
            <field name="name">I</field>
            <field name="validate_supplier">False</field>

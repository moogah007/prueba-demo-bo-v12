        <record id="account_denomination_m" model="account.denomination">
            <field name="name">M</field>
            <field name="validate_supplier">True</field>
            <field name="description">Comprobantes tipo M</field>
            <field name="vat_discriminated">True</field>
        </record>

        <record id="account_denomination_o" model="account.denomination">
            <field name="name">O</field>
            <field name="validate_supplier">False</field>
            <field name="description">Otros comprobantes</field>
        </record>

    </data>

</odoo>


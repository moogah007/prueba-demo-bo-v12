<odoo>
    <data noupdate="1">

        <record id="seq_journal_cobros_y_pagos" model="ir.sequence">
            <field name="name">Cobros y Pagos</field>
            <field name="code">journal.cyp</field>
            <field name="prefix">CYP/%(range_year)s/</field>
            <field name="padding">4</field>
        </record>

        <record model="account.journal" id="l10n_ar_account_payment.journal_cobros_y_pagos">
            <field name="name">Cobros y Pagos</field>
            <field name="code">CYP</field>
            <field name="sequence_id" ref="seq_journal_cobros_y_pagos"/>
            <field name="default_debit_account_id" ref="l10n_ar.1_caja_en_pesos"/>
            <field name="default_credit_account_id" ref="l10n_ar.1_caja_en_pesos"/>
            <field name="type">bank</field>
        </record>

    </data>
</odoo>


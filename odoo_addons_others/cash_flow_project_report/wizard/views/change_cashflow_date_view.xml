<odoo>

    <data>

        <record id="change_cashflow_date_form_view" model="ir.ui.view">
            <field name="name">change_cashflow_date_form_view</field>
            <field name="model">change.cashflow.date</field>
            <field name="arch" type="xml">
                <form string="">
                    <group>
                        <group>
                            <field name="date"/>
                        </group>
                        <group>
                            <field name="line_id" widget="selection"/>
                        </group>
                    </group>
                    <footer>
                        <button name="change_date"
                                string="Cambiar"
                                type="object"
                                class="btn-primary"
                        /> o
                        <button string="Cancelar"
                                class="btn-default"
                                special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>

</odoo>

